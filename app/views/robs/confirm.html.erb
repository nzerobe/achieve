

<h3>Comfirm</h3>


<%= form_with(model: @rob, url: robs_path, local: true) do |form| %>
     <%= form.hidden_field :title %>
  <%= form.hidden_field :content %>

  <p>Title:<%= @rob.title %></p>
  <p>Body:<%= @rob.content %></p>
   <p>Body:<%= image_tag(@rob.image.url(:thumb)) %></p>
   <%= hidden_field_tag :"cache[image]",@rob.image.cache_name%>
  <%= form.submit  "submit" %>
<% end %>

<%= form_with(model: @rob, url: new_rob_path, local: true, method: 'get') do |form| %>
  <%= form.hidden_field :title %>
  <%= form.hidden_field :content %>
  <%= form.hidden_field :image %>

  <%= form.submit "Back", name: 'back' %>
<% end %>




